<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="popup:org.eclipse.ui.popup.any">
         <command
               commandId="hiphopsplugin.startSafetyAnalysisHandler"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeMenuSelection">
                  <iterate>
                     <adapt
                           type="org.eclipse.core.resources.IFile">
                        <test
                              property="org.eclipse.core.resources.name"
                              value="*.uml">
                        </test>
                     </adapt>
                  </iterate>
                  <count
                        value="1">
                  </count>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="SafetyAnalysisPlugin.menuContribution1">
      </menuContribution>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="hiphopsplugin.startSafetyAnalysisHandler"
            name="Start safety analysis in HipHops">
      </command>
   </extension>
   <extension
         id="ATESST2"
         name="ATESST2"
         point="org.eclipse.ui.handlers">
      <handler
            class="hiphopsplugin.StartSafetyAnalysisHandler"
            commandId="hiphopsplugin.startSafetyAnalysisHandler">
      </handler>
   </extension>

</plugin>
